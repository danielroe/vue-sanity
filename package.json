{
  "name": "vue-sanity",
  "version": "0.8.0",
  "description": "Sanity integration for VueJS",
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "sanity",
    "composition-api",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/vue-sanity",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Roe <daniel@roe.dev>",
      "url": "https://github.com/danielroe"
    }
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf lib/*",
    "compile": "siroc build",
    "lint": "run-s lint:all:*",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "release": "release-it",
    "test": "run-s test:*",
    "test:types": "tsd",
    "test:unit": "jest"
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "dependencies": {
    "minify-groq": "^1.0.0",
    "sanity-typed-queries": "^0.6.11"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^1.1.4",
    "@sanity/client": "^1.149.18",
    "@sanity/image-url": "^0.140.19",
    "@siroc/eslint-config": "^0.2.0",
    "@siroc/jest-preset": "^0.2.0",
    "@types/jest": "^26.0.9",
    "@types/jsdom": "^16.2.3",
    "@types/memory-fs": "^0.3.2",
    "@vue/composition-api": "^0.6.7",
    "@vue/test-utils": "^1.0.3",
    "codecov": "^3.7.2",
    "flush-promises": "^1.0.2",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "memory-fs": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "release-it": "13.6.6",
    "siroc": "^0.3.0",
    "tsd": "^0.13.1",
    "typescript": "^3.9.7",
    "vue": "2",
    "vue-server-renderer": "^2.6.11",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.6.7",
    "vue": "^2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "tsd": {
    "directory": "test/tsd",
    "compilerOptions": {
      "rootDir": "."
    }
  }
}
