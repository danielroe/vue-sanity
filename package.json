{
  "name": "vue-sanity",
  "version": "0.10.2",
  "description": "Sanity integration for VueJS",
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "sanity",
    "composition-api",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/vue-sanity",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Roe <daniel@roe.dev>",
      "url": "https://github.com/danielroe"
    }
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf lib/*",
    "compile": "siroc build",
    "lint": "yarn lint:all:eslint && yarn lint:all:prettier",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "release": "release-it",
    "test": "yarn test:unit",
    "test:unit": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "dependencies": {
    "minify-groq": "^1.0.0",
    "sanity-typed-queries": "^0.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@release-it/conventional-changelog": "^2.0.0",
    "@sanity/client": "^1.150.7",
    "@sanity/image-url": "^0.140.19",
    "@siroc/eslint-config": "^0.2.0",
    "@siroc/jest-preset": "^0.2.0",
    "@types/jest": "^26.0.14",
    "@types/jsdom": "^16.2.4",
    "@types/memory-fs": "^0.3.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@vue/composition-api": "^1.0.0-beta.14",
    "@vue/test-utils": "^1.1.0",
    "codecov": "^3.7.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "expect-type": "^0.7.11",
    "flush-promises": "^1.0.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "memory-fs": "^0.5.0",
    "prettier": "^2.1.2",
    "release-it": "14.0.3",
    "siroc": "^0.3.3",
    "typescript": "^4.0.3",
    "vue": "2.6.12",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.14",
    "vue": "^2"
  }
}
