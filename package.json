{
  "name": "vue-sanity",
  "version": "0.11.0",
  "description": "Sanity integration for Vue",
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "sanity",
    "composition-api",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/vue-sanity",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "siroc build",
    "lint": "yarn lint:all:eslint && yarn lint:all:prettier",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build",
    "prepublishOnly": "yarn pinst --disable",
    "release": "release-it",
    "test": "yarn test:unit",
    "test:unit": "jest",
    "postinstall": "husky install",
    "postpublish": "yarn pinst --enable"
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "dependencies": {
    "@sanity/client": "^2.15.0",
    "@sanity/image-url": "^0.140.22",
    "minify-groq": "^1.0.0",
    "sanity-typed-queries": "^0.7.5",
    "vue-demi": "^0.11.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@jest/test-sequencer": "^27.0.6",
    "@release-it/conventional-changelog": "^3.3.0",
    "@siroc/eslint-config": "^0.2.0",
    "@types/jest": "^27.0.1",
    "@types/jsdom": "^16.2.13",
    "@types/memory-fs": "^0.3.3",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "@vue/composition-api": "1.1.3",
    "@vue/test-utils": "^1.2.2",
    "codecov": "^3.8.3",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect-type": "^0.12.0",
    "flush-promises": "^1.0.2",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-environment-jsdom": "^27.0.6",
    "jest-environment-node": "^27.0.6",
    "lint-staged": "^11.1.2",
    "memory-fs": "^0.5.0",
    "pinst": "^2.1.6",
    "prettier": "^2.3.2",
    "release-it": "14.11.5",
    "semver": "^7.3.5",
    "siroc": "^0.15.0",
    "typescript": "^4.3.5",
    "vue": "2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "@vue/composition-api": "1.1.3",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}
