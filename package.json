{
  "name": "vue-sanity",
  "version": "0.12.0",
  "description": "Sanity integration for Vue",
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "sanity",
    "composition-api",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/vue-sanity",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "postinstall": "husky install",
    "lint": "npm run lint:all:eslint && npm run lint:all:prettier",
    "lint:all:eslint": "npm run lint:eslint -- --ext .js,.ts,.vue .",
    "lint:all:prettier": "npm run lint:prettier -- \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "release-it",
    "test": "vitest"
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "dependencies": {
    "@sanity/client": "^3.3.2",
    "@sanity/image-url": "^1.0.1",
    "minify-groq": "^1.0.0",
    "sanity-typed-queries": "^0.9.0",
    "vue-demi": "^0.13.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "5.1.1",
    "@types/memory-fs": "^0.3.3",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "@vitest/coverage-c8": "0.28.1",
    "@vue/composition-api": "1.7.1",
    "@vue/test-utils": "^1.3.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "expect-type": "^0.15.0",
    "flush-promises": "^1.0.2",
    "happy-dom": "8.1.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "memory-fs": "^0.5.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "release-it": "15.6.0",
    "semver": "^7.3.7",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.2",
    "vite": "4.0.4",
    "vitest": "0.28.1",
    "vue": "2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "@vue/composition-api": "1.7.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "packageManager": "pnpm@7.25.1"
}
