{
  "name": "vue-sanity",
  "version": "0.12.0",
  "description": "Sanity integration for Vue",
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "sanity",
    "composition-api",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/vue-sanity",
  "license": "MIT",
  "author": {
    "name": "Daniel Roe <daniel@roe.dev>",
    "url": "https://github.com/danielroe"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "dist/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "unbuild",
    "postinstall": "husky install",
    "lint": "npm run lint:all:eslint && npm run lint:all:prettier",
    "lint:all:eslint": "npm run lint:eslint -- --ext .js,.ts,.vue .",
    "lint:all:prettier": "npm run lint:prettier -- \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --log-level warn",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "release-it",
    "test": "vitest"
  },
  "resolutions": {
    "minimist": ">=1.2.8"
  },
  "dependencies": {
    "@sanity/client": "^6.21.3",
    "@sanity/image-url": "^1.0.2",
    "minify-groq": "^1.0.0",
    "sanity-typed-queries": "^0.9.5",
    "vue-demi": "^0.14.10"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "8.0.1",
    "@types/memory-fs": "0.3.7",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "@vitest/coverage-v8": "2.0.5",
    "@vue/composition-api": "1.7.2",
    "@vue/test-utils": "1.3.6",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "eslint": "9.9.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "expect-type": "0.20.0",
    "flush-promises": "1.0.2",
    "happy-dom": "15.4.2",
    "husky": "9.1.5",
    "lint-staged": "15.2.9",
    "memory-fs": "0.5.0",
    "pinst": "3.0.0",
    "prettier": "3.3.3",
    "release-it": "17.6.0",
    "semver": "7.6.3",
    "typescript": "5.2.2",
    "unbuild": "0.7.2",
    "vite": "5.4.2",
    "vitest": "2.0.5",
    "vue": "2.6.14",
    "vue-server-renderer": "2.6.14",
    "vue-template-compiler": "2.6.14"
  },
  "peerDependencies": {
    "@vue/composition-api": "1.7.2",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.9.0"
}
