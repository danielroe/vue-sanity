{
  "name": "vue-sanity",
  "version": "0.10.4",
  "description": "Sanity integration for VueJS",
  "keywords": [
    "vue",
    "vuejs",
    "nuxt",
    "sanity",
    "composition-api",
    "typescript",
    "javascript"
  ],
  "repository": "danielroe/vue-sanity",
  "license": "MIT",
  "contributors": [
    {
      "name": "Daniel Roe <daniel@roe.dev>",
      "url": "https://github.com/danielroe"
    }
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.es.js"
    },
    "./package.json": "./package.json"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "lib/index.d.ts",
    "!**/*.map"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf lib/*",
    "compile": "siroc build",
    "postinstall": "husky install",
    "lint": "yarn lint:all:eslint && yarn lint:all:prettier",
    "lint:all:eslint": "yarn lint:eslint --ext .js,.ts,.vue .",
    "lint:all:prettier": "yarn lint:prettier \"**/*.{js,json,ts}\"",
    "lint:eslint": "eslint --fix",
    "lint:prettier": "prettier --write --loglevel warn",
    "prepare": "yarn build",
    "prepublishOnly": "yarn lint && yarn test",
    "release": "release-it",
    "test": "yarn test:unit",
    "test:unit": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "minimist": ">=1.2.5"
  },
  "dependencies": {
    "minify-groq": "^1.0.0",
    "sanity-typed-queries": "^0.7.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.9",
    "@release-it/conventional-changelog": "^2.0.1",
    "@sanity/client": "^2.2.6",
    "@sanity/image-url": "^0.140.22",
    "@siroc/eslint-config": "^0.2.0",
    "@siroc/jest-preset": "^0.2.0",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/memory-fs": "^0.3.2",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vue/composition-api": "1.0.0-rc.3",
    "@vue/test-utils": "^1.1.3",
    "codecov": "^3.8.1",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.7",
    "eslint-plugin-prettier": "^3.3.1",
    "expect-type": "^0.11.0",
    "flush-promises": "^1.0.2",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "memory-fs": "^0.5.0",
    "prettier": "^2.2.1",
    "release-it": "14.4.1",
    "semver": "^7.3.4",
    "siroc": "^0.7.0",
    "typescript": "^4.2.3",
    "vue": "2.6.12",
    "vue-server-renderer": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "@vue/composition-api": "1.0.0-rc.2",
    "vue": "^2"
  }
}
